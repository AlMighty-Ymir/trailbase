{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "baseUrl": "./",
    "paths": {
      "@/*": ["./src/*"],
      "@assets/*": ["../assets/*"],
      "@examples/*": ["./examples/*"],
      "@root/*": ["../*"]
    }
  },
  "exclude": ["dist", "node_modules", "public"]
}
