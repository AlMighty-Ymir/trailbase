{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "baseUrl": "./",
    "paths": {
      "@/*": ["./src/*"],
      "@assets/*": ["../assets/*"],
      "@common/*": ["../ui/common/*"]
    }
  },
  "exclude": [
    "dist",
    "node_modules",
    "public"
  ]
}
